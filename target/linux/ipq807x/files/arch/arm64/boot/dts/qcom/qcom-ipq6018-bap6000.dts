	// 		pins = "gpio69", "gpio71", "gpio72";
	// 		function = "blsp1_spi";
	// 		drive-strength = <8>;
	// 		bias-pull-down;
	// 	};
	// 	spi_cs {
	// 		pins = "gpio70";
	// 		function = "blsp1_spi";
	// 		drive-strength = <8>;
	// 		bias-disable;
	// 	};
	// 	quartz_interrupt {
